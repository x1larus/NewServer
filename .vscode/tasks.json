{
	"version": "2.0.0",
	"tasks": [
		{
			"type": "cppbuild",
			"label": "newserver",
			"command": "/usr/bin/g++",
			"args": [
				"-g",
				"${fileDirname}/main.cc",
				"${fileDirname}/socket_communication.cc",
				"${fileDirname}/encode.cc",
				"-o",
				"${fileDirname}/newserver",
				"-pthread",
				"-Wall", 
				"-Wextra", 
				"-std=c++17"
			],
			"options": {
				"cwd": "/usr/bin"
			},
			"problemMatcher": [
				"$gcc"
			],
			"group": "build",
			"detail": "compiler: /usr/bin/g++"
		}
	]
}